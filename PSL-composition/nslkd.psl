spec NSL;KD is
    composing NSL ; KD .

    A ;* B : ANAME1 |-> BNAME,  BNAME |-> ANAME, 
        K |-> h(n(ANAME, r) ; NB) . 
    B ;* A : ANAME |-> BNAME, BNAME |-> ANAME1, 
        K |-> h(NA1 ; n(BNAME, r1)) .

    A ;* A : A |-> A,  B |-> B, 
        K |-> h(n(A, r) ; NB) .     
    B ;* B : A |-> A1, B |-> B, 
        K |-> h(NA1 ; n(B, r1)) .

ends
